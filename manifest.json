{
    "name": "SEO Stuff",
    "description": "SEO extension that shows html tags and google search data from sistrix.de",
    "version": "0.8",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "seo_extension_icon.png",
        "48": "seo_extension_icon.png",
        "128": "seo_extension_icon.png"
    }
}